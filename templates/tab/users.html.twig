{% extends "base.html.twig" %}
{% block content %}
    <table class="table">
        <tr class="table-active">
            <td>Firstname</td>
            <td>Name</td>
            <td>Age</td>
        </tr>
        {% for user in users %}
        <tr class="table-{% if loop.index is odd %}info{% else %}success{% endif %}">
            <td>{{user.firstname}}</td>
            <td>{{user.name}}</td>
            <td>{{user.age}}</td>
        </tr>
        {% endfor %}
    </table>
{% endblock %}